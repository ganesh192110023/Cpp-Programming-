#include<iostream>
using namespace std;
int main()
{
	int a ,b;
	cout<<"Enter the 1st number=";
	cin>>a;
	cout<<"enter the 2nd number=";
	cin>>b;
	cout<<"addition:"<<a+b<<endl;
	cout<<"subtraction:"<<a-b<<endl;
	cout<<"multiplication:"<<a*b<<endl;
	cout<<"divison:"<<a/b<<endl;
	cout<<"modulus:"<<a%b;
}
