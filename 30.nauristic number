#include<iostream>
using namespace std;
int main(){
    int n,m,rem,sum=0,dig=0;
    cout  <<"enter the number:";
    cin  >>n;
    m=n;
    while(n!=0)
    {
        dig++;
        n=n/10;
    }
    n=m;
    while(n!=0)
    {
        rem=n%10;
        sum=sum+rem*rem*rem;
        n=n/10;
    }
    if(sum=m)
    {
        cout  <<"is an narcistic number";
    }
    else
    {
        cout  <<"is not an narcistic number";
    }
}
