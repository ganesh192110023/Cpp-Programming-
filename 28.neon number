#include<iostream>
#include<cmath>
using namespace std;
int main(){
    int n,m;
    int sum=0,rem;
    cout<<"enter the number:";
    cin>>n;
    m=n*n;
    while(m!=0)
    {
        rem=m%10;
        sum=sum+rem;
        m=m/10;
    }
    if(sum==n)
    {
        cout  <<"neon number";
    }
    else
    {
        cout  <<"not a neon number";
    }
    
}
    
